     *
     */
    [local]
    interface ITypeAliasInfo {
        /**
        *
        * Get type alias name.
        *
        * @param
        *      name [out] Output type alias name.
        *
        * @return
        *      Returns NOERROR if success, otherwise it returns the corresponding error code.
        */
        GetName(
            [out] String* name);

        /**
        *
