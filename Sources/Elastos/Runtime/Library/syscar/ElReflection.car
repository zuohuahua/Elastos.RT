        GetEnumInfo(
            [out] IEnumInfo** enumInfo);

        /**
        *
        * Get the value of enumeration item.
        *
        * @param
        *      value [out] Output the value of enumeration item.
        *
        * @return
        *      Returns NOERROR if success, otherwise it returns the corresponding error code.
        */
